{
    "name": "auto-all-antigravity",
    "displayName": "AUTO ALL AntiGravity",
    "description": "Automated accept workflow for AntiGravity. Inspired by open-source community.",
    "version": "1.0.0",
    "publisher": "auto-all-antigravity",
    "icon": "media/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ai-dev-2024/AUTO-ALL-AntiGravity"
    },
    "categories": [
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "antigravity",
        "ai",
        "auto-accept"
    ],
    "scripts": {
        "test": "node test/integration-test.js",
        "compile": "esbuild extension.js --bundle --outfile=dist/extension.js --external:vscode --platform=node --format=cjs",
        "vscode:prepublish": "npm run compile",
        "package": "npx vsce package"
    },
    "engines": {
        "vscode": "^1.75.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "auto-accept.toggle",
                "title": "Auto Accept: Toggle ON/OFF"
            },
            {
                "command": "auto-accept.toggleBackground",
                "title": "Auto Accept ALL: Toggle Multi-Tab Mode"
            },
            {
                "command": "auto-accept.openSettings",
                "title": "Auto Accept ALL: Settings"
            }
        ]
    },
    "dependencies": {
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "esbuild": "^0.27.1"
    }
}